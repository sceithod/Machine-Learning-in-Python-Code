from pywt import wavedec



coeffs = wavedec([1,2,3,4,5,6,7,8], 'db1', level=2)

name = 'db4'; 
waveletFunction='db8';
% Compute the corresponding scaling filter. 

[C,L] = wavedec(buttLoop,5,'db8');
cD1 = detcoef(C,L,1);                   %NOISY
cD2 = detcoef(C,L,2);                  %Gamma
cD3 = detcoef(C,L,3);                   %Beta
cD4 = detcoef(C,L,4);                   %Alpha
cD5 = detcoef(C,L,5);                   %Delta
cA5 = appcoef(C,L,waveletFunction,5);   %Theta
figure,title('Feature Extraction');subplot(6,1,1)
plot(cA5)
title(' Theta')
subplot(6,1,2)
plot(cD5)
title('  Delta')
subplot(6,1,3)
plot(cD4)
title('  Alpha')
subplot(6,1,4)
plot(cD3)
title('  Beta')
subplot(6,1,5)
plot(cD2)
title('  Gamma')